{
  "manifest_version": 3,
  "name": "FSR Dev Extension",
  "description": "Development tools for ForeSee Web SDK",
  "author": "Dream Team for America",
  "version": "1.1",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "16": "assets/favicon-16x16.png",
    "48": "assets/favicon-48x48.png",
    "128": "assets/favicon-128x128.png",
    "196": "assets/favicon-196x196.png"
  },
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "ForeSee Development Extension",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "/resource.js"
  ],
  "permissions": [
    "webRequest",
    "tabs",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "css": [],
      "js": [
        "content.js"
      ]
    }
  ]
}